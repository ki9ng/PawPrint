[Unit]
Description=Pawprint APRS Web Interface for KI9NG-10
After=network.target
Wants=direwolf.service

[Service]
Type=simple
User=asterisk
WorkingDirectory=/opt/pawprint
ExecStart=/opt/pawprint/venv/bin/python /opt/pawprint/app.py
Restart=on-failure
RestartSec=15
StandardOutput=journal
StandardError=journal
SyslogIdentifier=pawprint

[Install]
WantedBy=multi-user.target
